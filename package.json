{
  "name": "semantic-merge",
  "version": "0.1.0",
  "author": {
    "name": "Robert Rossmann",
    "email": "rr.rossmann@me.com"
  },
  "devDependencies": {
    "coveralls": "^2.11.2",
    "eslint": "^0.15.0",
    "istanbul": "^0.3.2",
    "jsdoc": "3.3.0-beta1",
    "mocha": "^2.0.1",
    "mocha-lcov-reporter": "^0.0.1",
    "should": "^5"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "homepage": "https://github.com/Dreamscapes/semantic-merge",
  "eslintConfig": {
    "env": {
      "node": true,
      "mocha": true
    },
    "rules": {
      "brace-style": 2,
      "comma-style": [2, "first"],
      "consistent-return": 1,
      "consistent-this": [2, "that"],
      "curly": [2, "multi"],
      "default-case": 2,
      "func-style": [2, "declaration"],
      "handle-callback-err": [1, "^.*(e|E)rr"],
      "indent": [1, 2, {
        "indentSwitchCase": true
      }],
      "max-len": [2, 100, 2],
      "max-nested-callbacks": [1, 4],
      "max-params": [1, 3],
      "no-else-return": 2,
      "no-eq-null": 2,
      "no-floating-decimal": 1,
      "no-mixed-requires": 0,
      "no-multiple-empty-lines": [2, {
        "max": 2
      }],
      "no-nested-ternary": 2,
      "no-process-exit": 1,
      "no-self-compare": 2,
      "no-space-before-semi": 0,  // We don't use semicolons and this keeps complaining
      "no-throw-literal": 2,
      "no-unused-expressions": 1,
      "no-use-before-define": [2, "nofunc"],
      "no-warning-comments": [1, {
        "terms": ["todo", "fixme"],
        "location": "start"
      }],
      "operator-assignment": [1, "always"],
      "quote-props": [2, "as-needed"],
      "quotes": [2, "single", "avoid-escape"],
      "semi": [2, "never"],
      "space-after-keywords": 2,
      "space-before-blocks": 2,
      "space-before-function-parentheses": 2,
      "space-in-brackets": [2, "always", {
        "singleValue": false,
        "arraysInArrays": false,
        "propertyName": false
      }],
      "space-in-parens": 2,
      "space-unary-ops": [1, {
        "words": true,
        "nonwords": true
      }],
      "spaced-line-comment": 2,
      "strict": [2, "global"],
      "wrap-iife": [2, "inside"],
      "wrap-regex": 2,
      "valid-jsdoc": [1, {
        "requireReturn": false,
        "requireReturnDescription": false
      }]
    }
  },
  "keywords": [],
  "main": "lib/merger.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/Dreamscapes/semantic-merge.git"
  },
  "scripts": {
    "test": "make lint test"
  }
}
